int main()
{
    glutInitDisplayMode(GLUT_SINGLE|GLUT_RGB);
    glutInitWindowSize(900,650);
    glutInitWindowPosition(400,200);
    glutCreateWindow("Mosque");
    init();
    glutDisplayFunc(myDisplay);
    glutMainLoop();
    return 0;
}
